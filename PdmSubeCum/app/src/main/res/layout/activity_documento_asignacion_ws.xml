<?xml version="1.0" encoding="utf-8"?>
<LinearLayout tools:context=".WebServices.DocumentoAsignacionWSActivity"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/LinearLayout1"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <LinearLayout android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">

        <Button
            android:id="@+id/button_documentoAsignacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="consultarMaximaAsignacion"
            android:text="@string/documento_asignacion" />

    </LinearLayout>

    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/textView_cantidad"
        android:text="@string/documento_asignacion_mas"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/textView3"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_weight="0.08"/>


</LinearLayout>
