<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:paddingTop="30dip"
    android:stretchColumns="1" >
    <TableRow>
        <TextView android:text="@string/idDocMov"/>
        <EditText
            android:id="@+id/editocMov"
            android:inputType="text"
            android:maxLength="7" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/idUnOrigen" />
        <EditText
            android:id="@+id/editUnOrigen"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/idUniDestino" />
        <EditText
            android:id="@+id/editUniDestino"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/idTipo" />
        <EditText
            android:id="@+id/editTipo"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/descTipoM" />
        <EditText
            android:id="@+id/editdescTipoM"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/fecha" />
        <EditText
            android:id="@+id/editfecha"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/isbnDoc" />
        <EditText
            android:id="@+id/editisbnDoc"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/idMovDetalle" />
        <EditText
            android:id="@+id/editidMovDetalle"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>
    <TableRow>
        <TextView android:text="@string/opcionesTM" />
    </TableRow>
    <TableRow>
        <Button
            android:onClick="insertar"
            android:text="@string/insertar" />
        <Button
            android:onClick="limpiarTexto"
            android:text="@string/limpiar" />
    </TableRow>
</TableLayout>